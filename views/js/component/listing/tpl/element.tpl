<div class="notif-element {{status}}">
    <div class="container-icon">
        <div class="shape">
            <span class="icon-{{type}}"/>
        </div>
    </div>
    <div class="container-text">
        <div class="label">{{label}}</div>
        <div class="time">{{time}}</div>
    </div>
    <div class="action-bar action-top">
        <span class="icon-bin"/>
    </div>
    <div class="action-bar action-bottom">
        <span data-role="notify" class="icon-preview"/>
        <span data-role="download" class="icon-download"/>
        <span data-role="report" class="icon-document"/>
    </div>
</div>